{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    },
    {
      "mode": "ts",
      "include": "**/*.ts",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    }
  ],
  "typesOverrides": {
    "date": "string",
    "int8": "BigInt"
  },
  "srcDir": "./src/",
  "dbUrl": "postgres://postgres:password@localhost/postgres"
}
