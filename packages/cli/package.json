    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "io-ts": "^2.1.2",
    "io-ts-reporters": "^1.0.0",
    "nunjucks": "3.2.3",
    "pascal-case": "^3.1.1",
    "ts-parse-database-url": "^1.0.3",
    "yargs": "^17.0.1"
